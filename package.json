{
  "name": "grimorio-ui",
  "sideEffects": false,
  "version": "6.1.1",
  "description": "React UI Kit that works like magic",
  "main": "lib/index.js",
  "scripts": {
    "cp:pack": "rm ../$APP/packages/grimorio-ui-*.tgz && ./node_modules/.bin/cpy grimorio-ui-*.tgz ../$APP/packages/",
    "cp": "./node_modules/.bin/cpy grimorio-ui-*.tgz ../$APP/packages/",
    "pack:build": "yarn build && yarn pack --filename grimorio-ui-${npm_package_version}.$THEME_ENV.tgz",
    "pack:full": "rm -f ./grimorio-ui-**.tgz && yarn pack:build",
    "build": "yarn build:images && yarn build:js",
    "build:js": "./node_modules/.bin/cross-env THEME_ENV=$THEME_ENV NODE_ENV=production BABEL_ENV=production babel source --out-dir lib && webpack --progress --config ./internals/webpack/webpack.config.js",
    "build:images": "./node_modules/.bin/cross-env NODE_ENV=production BABEL_ENV=production babel source/images --out-dir lib/images --copy-files",
    "lint": "run-p lint:*",
    "lint:styl": "stylint source/",
    "lint:js": "eslint source/",
    "precommit": "yarn lint",
    "release": "./node_modules/.bin/release-it",
    "storybook": "./node_modules/.bin/cross-env THEME_ENV=$THEME_ENV start-storybook -p 9000",
    "test": "./node_modules/.bin/cross-env NODE_ENV=testing BABEL_ENV=testing jest",
    "test:verb": "./node_modules/.bin/cross-env NODE_ENV=testing BABEL_ENV=testing jest --verbose",
    "test:tdd": "yarn test -- --watch",
    "test:coverage": "yarn test -- --coverage --coverageDirectory=coverage",
    "create:comp": "bash ./helpers/component/create.sh $COMP ./source/components"
  },
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0"
  },
  "directories": {
    "doc": "docs"
  },
  "author": "@b2wads",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": " 7.5.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "7.5.4",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "@dump247/storybook-state": "1.5.0",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-events": "5.1.9",
    "@storybook/addon-info": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addon-options": "5.1.9",
    "@storybook/addon-storysource": "5.1.9",
    "@storybook/addon-viewport": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-css-modules-transform": "1.5.0",
    "chai": "4.1.1",
    "child-process-es6-promise": "1.2.0",
    "cpy-cli": "2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.4",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.2.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "4.19.0",
    "eslint-config-google": "0.9.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "0.8.0",
    "jest": "24.1.0",
    "jest-css-modules": "1.1.0",
    "jest-css-modules-transform": "1.0.5",
    "jest-dot-reporter": "1.0.3",
    "jsdom": "9.12.0",
    "npm-run-all": "4.1.2",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.6",
    "precss": "2.0.0",
    "prettier": "1.3.1",
    "react-test-renderer": "16.3.1",
    "rimraf": "2.6.2",
    "style-loader": "0.17.0",
    "stylint": "1.5.9",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "svg-react-loader": "0.4.5",
    "webpack": "3.5.2",
    "webpack-bundle-analyzer": "3.3.2"
  },
  "dependencies": {
    "chart.js": "2.7.3",
    "chartjs-plugin-datalabels": "0.5.0",
    "classnames": "2.2.5",
    "moment": "2.22.1",
    "prop-types": "15.5.10",
    "react": "16.8.6",
    "react-css-modules": "4.7.2",
    "react-dates": "18.2.2",
    "react-dom": "16.8.6"
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "^helpers(.*)$": "<rootDir>/source/helpers/",
      "^components(.*)$": "<rootDir>/source/components/",
      "\\.(css|styl)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./internals/file-transformer.js"
    },
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest",
      "\\.(css|styl)$": "jest-css-modules-transform"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/source/**/*.spec.js"
    ],
    "setupTestFrameworkScriptFile": "./internals/test/setup.js",
    "coverageDirectory": ".reports/coverage",
    "cacheDirectory": "./tmp/",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
