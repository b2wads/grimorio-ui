{
  "name": "bitservices-theme",
  "sideEffects": false,
  "version": "3.3.2-beta.1",
  "description": "Bitservices UI React",
  "main": "lib/index.js",
  "scripts": {
    "cp:pack": "rm ../$APP/packages/bitservices-theme-*.tgz && ./node_modules/.bin/cpy bitservices-theme-*.tgz ../$APP/packages/",
    "pack:build": "yarn build && yarn pack --filename bitservices-theme-${npm_package_version}.$THEME_ENV.tgz",
    "pack:full": "rm -f ./bitservices-theme-**.tgz && yarn pack:build",
    "build": "yarn build:images && yarn build:js",
    "build:js": "./node_modules/.bin/cross-env THEME_ENV=$THEME_ENV NODE_ENV=production BABEL_ENV=production babel source --out-dir lib && webpack --progress --config ./internals/webpack/webpack.config.js",
    "build:images": "./node_modules/.bin/cross-env NODE_ENV=production BABEL_ENV=production babel source/images --out-dir lib/images --copy-files",
    "lint": "run-p lint:*",
    "lint:styl": "stylint source/",
    "lint:js": "eslint source/",
    "precommit": "yarn lint",
    "prepush": "yarn test",
    "release": "./node_modules/.bin/release-it",
    "storybook": "./node_modules/.bin/cross-env THEME_ENV=$THEME_ENV start-storybook -p 9000 -c ./internals/storybook",
    "test": "./node_modules/.bin/cross-env NODE_ENV=testing BABEL_ENV=testing jest",
    "test:verb": "./node_modules/.bin/cross-env NODE_ENV=testing BABEL_ENV=testing jest --verbose",
    "test:tdd": "yarn test -- --watch",
    "test:coverage": "yarn test -- --coverage --coverageDirectory=coverage"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0"
  },
  "directories": {
    "doc": "docs"
  },
  "author": "Bitservices",
  "contributors": [
    "Ana Azevedo <ana.fazevedo@b2wdigital.com> (http://)",
    "Deborah Miranda <dmiranda@b2wdigital.com> (http://)",
    "Lais Chagas <lais.chagas@b2wdigital.com> (http://)",
    "Hemerson Vianna <hemerson.lourenco@b2wdigital.com> (http://)"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.44",
    "@babel/plugin-proposal-export-default-from": "7.0.0-beta.44",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-react": "7.0.0-beta.44",
    "@babel/register": "7.0.0-beta.44",
    "@dump247/storybook-state": "1.5.0",
    "@storybook/addon-actions": "3.4.1",
    "@storybook/addon-events": "3.4.1",
    "@storybook/addon-info": "3.4.1",
    "@storybook/addon-knobs": "3.4.1",
    "@storybook/addon-options": "3.4.1",
    "@storybook/addons": "3.4.1",
    "@storybook/react": "3.4.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "7.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-css-modules-transform": "1.5.0",
    "chai": "4.1.1",
    "child-process-es6-promise": "1.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.4",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.0",
    "eslint-config-google": "0.9.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.5.1",
    "file-loader": "0.11.2",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "0.8.0",
    "jest": "22.4.3",
    "jest-css-modules": "1.1.0",
    "jest-css-modules-transform": "1.0.5",
    "jest-dot-reporter": "1.0.3",
    "jsdom": "9.12.0",
    "npm-run-all": "4.1.2",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.6",
    "precss": "2.0.0",
    "prettier": "1.3.1",
    "react-test-renderer": "16.3.1",
    "release-it": "7.3.0",
    "rimraf": "2.6.2",
    "style-loader": "0.17.0",
    "stylint": "1.5.9",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "svg-react-loader": "0.4.5",
    "webpack-bundle-analyzer": "3.0.3"
  },
  "dependencies": {
    "chart.js": "2.7.3",
    "classnames": "2.2.5",
    "cpy-cli": "2.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "lodash": "4.17.11",
    "moment": "2.22.1",
    "prop-types": "15.5.10",
    "react": "16.3.1",
    "react-css-modules": "4.7.2",
    "react-dates": "18.2.2",
    "react-dom": "16.3.3",
    "webpack": "3.5.2"
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "\\.(css|styl)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./internals/file-transformer.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(css|styl)$": "jest-css-modules-transform"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/source/**/*.spec.js"
    ],
    "setupTestFrameworkScriptFile": "./internals/test/setup.js",
    "coverageDirectory": ".reports/coverage",
    "reporters": [
      "jest-dot-reporter"
    ],
    "cacheDirectory": "./tmp/"
  }
}
